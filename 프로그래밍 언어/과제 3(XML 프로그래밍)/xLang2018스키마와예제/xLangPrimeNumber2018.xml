<?xml version="1.0" encoding="UTF-8"?>
<xLang18 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="xLang2018.xsd">
	<Author>Su-Hyun Lee</Author>
	<Function>
		<Fname>main</Fname>
		<Statement>
			<Input>
				<Vname>n</Vname>
			</Input>
			<Return>
				<Fun>
					<Fname>recursive</Fname>
					<Param>
						<Vname>n</Vname>
					</Param>
				</Fun>
			</Return>
		</Statement>
	</Function>
	<Function>
		<Fname>recursive</Fname>
		<Param>
			<Vname>n</Vname>
		</Param>
		<Statement>
			<If>
				<EQ>
					<Vname>n</Vname>
					<Num>1</Num>
				</EQ>
				<Then>
					<Return>
						<Num>0</Num>
					</Return>
				</Then>
				<Else>
					<If>
						<EQ>
							<Fun>
								<Fname>prime</Fname>
								<Param>
									<Vname>n</Vname>
								</Param>
							</Fun>
							<Num>1</Num>
						</EQ>
						<Then>
							<Output>
								<Vname>n</Vname>
							</Output>
						</Then>
					</If>
					<Return>
						<Fun>
							<Fname>recursive</Fname>
							<Param>
								<Sub>
									<Vname>n</Vname>
									<Num>1</Num>
								</Sub>
							</Param>
						</Fun>
					</Return>
				</Else>
			</If>
		</Statement>
	</Function>
	<Function>
		<Fname>prime</Fname>
		<Param>
			<Vname>i</Vname>
		</Param>
		<Statement>
			<Assign>
				<Vname>p</Vname>
				<Num>1</Num>
			</Assign>
			<Assign>
				<Vname>j</Vname>
				<Num>2</Num>
			</Assign>
			<While>
				<LT>
					<Vname>j</Vname>
					<Vname>i</Vname>
				</LT>
				<Do>
					<If>
						<EQ>
							<Mod>
								<Vname>i</Vname>
								<Vname>j</Vname>
							</Mod>
							<Num>0</Num>
						</EQ>
						<Then>
							<Assign>
								<Vname>p</Vname>
								<Num>0</Num>
							</Assign>
						</Then>
					</If>
					<Assign>
						<Vname>j</Vname>
						<Add>
							<Vname>j</Vname>
							<Num>1</Num>
						</Add>
					</Assign>
				</Do>
			</While>
			<Return>
				<Vname>p</Vname>
			</Return>
		</Statement>
	</Function>
</xLang18>
